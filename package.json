        "babel-cli": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "big-number": "^2.0.0",
        "bignumber.js": "^9.0.1",
        "bn.js": "^5.1.3",
        "chai": "^4.2.0",
        "crypto-js": "^3.1.9-1",
        "dayjs": "^1.9.4",
        "decimal.js": "^10.2.1",
        "delay": "^4.4.0",
        "dotenv": "^8.2.0",
        "ethers": "^5.3.1",
        "execution-time": "^1.4.1",
        "express": "^4.17.1",
